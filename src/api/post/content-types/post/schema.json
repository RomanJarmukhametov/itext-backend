{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "singularName": "post",
    "pluralName": "posts",
    "displayName": "Blog Posts",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "subtitle": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "content": {
      "type": "blocks"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "author": {
      "type": "enumeration",
      "enum": [
        "Роман Джармухаметов"
      ]
    },
    "category": {
      "type": "enumeration",
      "enum": [
        "Бизнес",
        "Юридические переводы",
        "Технические переводы",
        "Локализация",
        "Личные документы",
        "Советы клиентам",
        "Образование и научные переводы",
        "Технологии и будущее переводов",
        "Корпоративные переводы",
        "Обзор услуг iText "
      ]
    }
  }
}
